{
	"name": "mittvibes-cli",
	"version": "1.0.0",
	"description": "CLI tool to generate boilerplate for mittwald extensions",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
	"bin": {
		"mittvibes": "./dist/index.js"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsx src/index.tsx",
		"start": "node dist/index.js",
		"clean": "rm -rf dist",
		"check": "pnpm biome check .",
		"format": "pnpm biome format",
		"format:fix": "pnpm biome format --fix",
		"lint": "pnpm biome lint",
		"lint:fix": "pnpm biome lint --fix",
		"prepublishOnly": "pnpm clean && pnpm build"
	},
	"files": [
		"dist/**/*"
	],
	"keywords": [
		"mittwald",
		"extension",
		"boilerplate",
		"cli",
		"generator"
	],
	"author": "mittwald",
	"license": "MIT",
	"dependencies": {
		"@47ng/cloak": "^1.2.0",
		"@mittwald/api-client": "^4.230.0",
		"@types/react": "^19.1.15",
		"dotenv": "^16.4.5",
		"fs-extra": "^11.2.0",
		"get-port": "^7.1.0",
		"ink": "^6.3.1",
		"ink-select-input": "^6.2.0",
		"ink-text-input": "^6.0.0",
		"node-fetch": "^3.3.2",
		"open": "^10.2.0",
		"react": "^19.1.1",
		"yauzl": "^3.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.6",
		"@types/fs-extra": "^11.0.4",
		"@types/node": "^22.5.4",
		"@types/node-fetch": "^2.6.13",
		"@types/yauzl": "^2.10.3",
		"tsx": "^4.19.1",
		"typescript": "^5.6.2"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
